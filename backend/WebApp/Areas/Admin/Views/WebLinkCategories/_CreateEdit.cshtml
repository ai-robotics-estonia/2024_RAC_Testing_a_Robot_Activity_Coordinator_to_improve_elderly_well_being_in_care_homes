@model WebApp.Areas.Admin.ViewModels.WebLinkCategoryViewModel
<input type="hidden" asp-for="WebLinkCategory.CategoryDisplayName" value="{}">

<div class="form-group">
    <label asp-for="WebLinkCategory.CategoryName" class="control-label"></label>
    <input asp-for="WebLinkCategory.CategoryName" class="form-control" />
    <span asp-validation-for="WebLinkCategory.CategoryName" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="CategoryDisplayName" class="control-label"></label>
    <input asp-for="CategoryDisplayName" class="form-control" />
    <span asp-validation-for="CategoryDisplayName" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="WebLinkCategory.OrganizationId" class="control-label"></label>
    <select asp-for="WebLinkCategory.OrganizationId" class="form-select" asp-items="Model.OrganizationSelectList"></select>
</div>