@model WebApp.Areas.Admin.ViewModels.GreetingPhraseCategoryViewModel

<input type="hidden" asp-for="GreetingPhraseCategory.CategoryDisplayName" value="{}">
<div class="form-group">
    <label asp-for="GreetingPhraseCategory.CategoryName" class="control-label"></label>
    <input asp-for="GreetingPhraseCategory.CategoryName" class="form-control" />
    <span asp-validation-for="GreetingPhraseCategory.CategoryName" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="CategoryDisplayName" class="control-label"></label>
    <input asp-for="CategoryDisplayName" class="form-control" />
    <span asp-validation-for="CategoryDisplayName" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="GreetingPhraseCategory.OrganizationId" class="control-label"></label>
    <select asp-for="GreetingPhraseCategory.OrganizationId" class="form-select" asp-items="Model.OrganizationSelectList"></select>
</div>


