@model App.Domain.Article

<dl class="row">
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.ArticleCategory)
    </dt>
    <dd class="col-sm-10">
        @Model.ArticleCategory!.CategoryDisplayName (@Model.ArticleCategory!.CategoryName)
    </dd>

    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.PlainText)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.PlainText)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.DisplayText)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.DisplayText)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.RobotMapApp)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.RobotMapApp!.DisplayName)
    </dd>
</dl>
